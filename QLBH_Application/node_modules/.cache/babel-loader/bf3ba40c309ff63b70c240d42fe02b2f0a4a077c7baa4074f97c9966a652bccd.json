{"ast":null,"code":"import pageheader from \"@/components/page-header.vue\";\nimport { VCurrencyField } from \"vue-currency-input\";\nexport default {\n  name: \"PRODUCT-LIST\",\n  components: {\n    pageheader\n  },\n  directives: {\n    currency: VCurrencyField\n  },\n  data() {\n    return {\n      arrange: [{\n        title: 'Thứ tự mặc định',\n        value: 0\n      }, {\n        title: 'Mới nhất',\n        value: 1\n      }, {\n        title: 'Giá từ thấp đến cao',\n        value: 2\n      }, {\n        title: 'Giá từ cao đến thấp',\n        value: 3\n      }],\n      orderBy: null,\n      searchPrice: [0, 1000000],\n      productTypeData: [],\n      productData: []\n    };\n  },\n  watch: {\n    // Theo dõi sự thay đổi của query parameter productName\n    '$route.query.productName': function (newValue) {\n      this.productName = newValue; // Cập nhật giá trị của productName\n      // Gọi phương thức mỗi khi query thay đổi\n      this.GetProductType(newValue);\n      this.GetPriceProduct(newValue);\n      this.GetProduct(newValue);\n    }\n  },\n  created() {\n    this.GetProductType(\"\");\n    this.GetPriceProduct(\"\");\n    this.GetProduct(\"\");\n  },\n  methods: {\n    GetProductType(productName) {\n      this.$apiClient.get(`/ProductType/GetProductTypeByName?name=${productName}`).then(response => {\n        this.productTypeData = response.data;\n      });\n    },\n    GetPriceProduct(productName) {\n      this.$apiClient.get(`/Product/GetPrice?name=${productName}`).then(response => {\n        this.searchPrice[0] = response.data.priceFrom;\n        this.searchPrice[1] = response.data.priceTo;\n      });\n    },\n    GetProduct(productName) {\n      this.$apiClient.get(`/Product/FindProduct?name=${productName}`).then(response => {\n        this.productData = response.data;\n      });\n    }\n  }\n};","map":{"version":3,"names":["pageheader","VCurrencyField","name","components","directives","currency","data","arrange","title","value","orderBy","searchPrice","productTypeData","productData","watch","$route.query.productName","newValue","productName","GetProductType","GetPriceProduct","GetProduct","created","methods","$apiClient","get","then","response","priceFrom","priceTo"],"sources":["D:\\Du_An\\BanHang_ThietBiDienThu\\QLBH_Application\\src\\views\\SearchForProducts\\ListProduct.vue"],"sourcesContent":["<script>\r\n    import pageheader from \"@/components/page-header.vue\"\r\n    import { VCurrencyField } from \"vue-currency-input\";\r\n\r\n    export default {\r\n        name: \"PRODUCT-LIST\",\r\n        components: {\r\n            pageheader\r\n        },\r\n        directives: {\r\n            currency: VCurrencyField,\r\n        },\r\n        data(){\r\n            return{\r\n                arrange:[\r\n                    {title: 'Thứ tự mặc định', value: 0},\r\n                    {title: 'Mới nhất', value: 1},\r\n                    {title: 'Giá từ thấp đến cao', value: 2},\r\n                    {title: 'Giá từ cao đến thấp', value: 3},\r\n                ],\r\n                orderBy: null,\r\n                searchPrice: [0,1000000],\r\n                productTypeData: [],\r\n                productData:[\r\n\r\n                ]\r\n            }\r\n        },\r\n        watch: {\r\n            // Theo dõi sự thay đổi của query parameter productName\r\n            '$route.query.productName': function(newValue) {\r\n                this.productName = newValue; // Cập nhật giá trị của productName\r\n                // Gọi phương thức mỗi khi query thay đổi\r\n                this.GetProductType(newValue); \r\n                this.GetPriceProduct(newValue);\r\n                this.GetProduct(newValue);\r\n            }\r\n        },\r\n        created(){\r\n            this.GetProductType(\"\");\r\n            this.GetPriceProduct(\"\");\r\n            this.GetProduct(\"\");\r\n        },\r\n        methods:{\r\n            GetProductType(productName){\r\n                this.$apiClient.get(`/ProductType/GetProductTypeByName?name=${productName}`)\r\n                        .then(response=>{\r\n                            this.productTypeData = response.data;\r\n                        })\r\n            },\r\n            GetPriceProduct(productName){\r\n                this.$apiClient.get(`/Product/GetPrice?name=${productName}`)\r\n                        .then(response=>{\r\n                            this.searchPrice[0] = response.data.priceFrom;\r\n                            this.searchPrice[1] = response.data.priceTo;\r\n                        })\r\n            },\r\n            GetProduct(productName){\r\n                this.$apiClient.get(`/Product/FindProduct?name=${productName}`)\r\n                        .then(response=>{\r\n                            this.productData = response.data;\r\n\r\n                        })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<template>\r\n    <BRow>\r\n        <BCol class=\"col-xl-2\">\r\n            <pageheader title=\"\" pageTitle=\"Sản phẩm\" />\r\n        </BCol>\r\n        <BCol class=\"col-xl-3 p-3\">\r\n            <v-autocomplete\r\n                clearable\r\n                label=\"Loại sản phẩm\"\r\n                :items=\"productTypeData\"\r\n                item-title=\"name\"\r\n                item-value=\"id\"\r\n                v-model=\"orderBy\"\r\n                variant=\"outlined\"\r\n                hide-details>\r\n            </v-autocomplete>\r\n        </BCol>\r\n        <BCol class=\"col-xl-3 p-3\">\r\n            <div class=\"form-group m-0\">\r\n                <label class=\"form-label\">Giá phòng</label>\r\n                <v-range-slider\r\n                    v-model=\"searchPrice\"\r\n                    range\r\n                    min=\"0\"\r\n                    max=\"10000000\"\r\n                    step=\"1000\"\r\n                    color=\"primary\"\r\n                    hide-details\r\n                ></v-range-slider>\r\n                <BRow>\r\n                    <BCol class=\"col-6\">\r\n                        <v-text-field\r\n                            v-model=\"searchPrice[0]\"\r\n                            v-currency\r\n                            currency=\"VND\"\r\n                            locale=\"vi-VN\"\r\n                            label=\"Từ\"\r\n                            hide-details\r\n                        ></v-text-field>\r\n                    </BCol>\r\n                    <BCol class=\"col-6\">\r\n                        <v-text-field\r\n                            label=\"Đến\"\r\n                            v-model=\"searchPrice[1]\"\r\n                            :type=\"'text'\"\r\n                            hide-details\r\n                            suffix=\"đ\"\r\n                        ></v-text-field>\r\n                    </BCol>\r\n                </BRow>\r\n            </div>\r\n        </BCol>\r\n        <BCol class=\"col-xl-3 p-3\">\r\n            <v-select\r\n                clearable\r\n                label=\"Sắp xếp\"\r\n                :items=\"arrange\"\r\n                item-title=\"title\"\r\n                item-value=\"value\"\r\n                v-model=\"orderBy\"\r\n                variant=\"outlined\"\r\n                hide-details>\r\n            </v-select>\r\n        </BCol>\r\n        <BCol class=\"col-xl-1 d-flex align-center\">\r\n            <BButton type=\"button\" variant=\"primary\" @click=\"saveUpdate()\" >Lọc</BButton>\r\n        </BCol>\r\n    </BRow>\r\n    <BRow>\r\n        <div v-for=\"(item,index) in productData\" :key=\"index\" class=\"col-xl-3 col-12 p-2\">\r\n            <router-link >\r\n                <div class=\"d-flex w-100 justify-center\">\r\n                    <img :src=\"item.pathImg\" width=\"280\" height=\"280\" alt=\"\">\r\n                </div>\r\n                <div class=\"text-center mt-3\">\r\n                    <p style=\"font-size: 18px !important;\">{{ item.name }}</p>\r\n                    <h3 style=\"color: red;\">{{ item.price.toLocaleString(\"vi-vn\") }} VNĐ</h3>\r\n                </div>\r\n            </router-link>\r\n        </div>\r\n    </BRow>\r\n</template>"],"mappings":"AACI,OAAOA,UAAS,MAAO,8BAA6B;AACpD,SAASC,cAAa,QAAS,oBAAoB;AAEnD,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,UAAU,EAAE;IACRC,QAAQ,EAAEJ;EACd,CAAC;EACDK,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,OAAO,EAAC,CACJ;QAACC,KAAK,EAAE,iBAAiB;QAAEC,KAAK,EAAE;MAAC,CAAC,EACpC;QAACD,KAAK,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAC,CAAC,EAC7B;QAACD,KAAK,EAAE,qBAAqB;QAAEC,KAAK,EAAE;MAAC,CAAC,EACxC;QAACD,KAAK,EAAE,qBAAqB;QAAEC,KAAK,EAAE;MAAC,CAAC,CAC3C;MACDC,OAAO,EAAE,IAAI;MACbC,WAAW,EAAE,CAAC,CAAC,EAAC,OAAO,CAAC;MACxBC,eAAe,EAAE,EAAE;MACnBC,WAAW,EAAC;IAGhB;EACJ,CAAC;EACDC,KAAK,EAAE;IACH;IACA,0BAA0B,EAAE,SAAAC,CAASC,QAAQ,EAAE;MAC3C,IAAI,CAACC,WAAU,GAAID,QAAQ,EAAE;MAC7B;MACA,IAAI,CAACE,cAAc,CAACF,QAAQ,CAAC;MAC7B,IAAI,CAACG,eAAe,CAACH,QAAQ,CAAC;MAC9B,IAAI,CAACI,UAAU,CAACJ,QAAQ,CAAC;IAC7B;EACJ,CAAC;EACDK,OAAOA,CAAA,EAAE;IACL,IAAI,CAACH,cAAc,CAAC,EAAE,CAAC;IACvB,IAAI,CAACC,eAAe,CAAC,EAAE,CAAC;IACxB,IAAI,CAACC,UAAU,CAAC,EAAE,CAAC;EACvB,CAAC;EACDE,OAAO,EAAC;IACJJ,cAAcA,CAACD,WAAW,EAAC;MACvB,IAAI,CAACM,UAAU,CAACC,GAAG,CAAC,0CAA0CP,WAAW,EAAE,EAClEQ,IAAI,CAACC,QAAQ,IAAE;QACZ,IAAI,CAACd,eAAc,GAAIc,QAAQ,CAACpB,IAAI;MACxC,CAAC;IACb,CAAC;IACDa,eAAeA,CAACF,WAAW,EAAC;MACxB,IAAI,CAACM,UAAU,CAACC,GAAG,CAAC,0BAA0BP,WAAW,EAAE,EAClDQ,IAAI,CAACC,QAAQ,IAAE;QACZ,IAAI,CAACf,WAAW,CAAC,CAAC,IAAIe,QAAQ,CAACpB,IAAI,CAACqB,SAAS;QAC7C,IAAI,CAAChB,WAAW,CAAC,CAAC,IAAIe,QAAQ,CAACpB,IAAI,CAACsB,OAAO;MAC/C,CAAC;IACb,CAAC;IACDR,UAAUA,CAACH,WAAW,EAAC;MACnB,IAAI,CAACM,UAAU,CAACC,GAAG,CAAC,6BAA6BP,WAAW,EAAE,EACrDQ,IAAI,CAACC,QAAQ,IAAE;QACZ,IAAI,CAACb,WAAU,GAAIa,QAAQ,CAACpB,IAAI;MAEpC,CAAC;IACb;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}