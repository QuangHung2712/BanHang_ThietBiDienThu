{"ast":null,"code":"import { Swiper, SwiperSlide } from 'swiper/vue';\nexport default {\n  data() {\n    return {\n      modules: [Navigation, Autoplay]\n    };\n  },\n  name: \"LANDING\",\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  computed: {},\n  created() {},\n  methods: {}\n};","map":{"version":3,"names":["Swiper","SwiperSlide","data","modules","Navigation","Autoplay","name","components","computed","created","methods"],"sources":["D:\\Du_An\\BanHang_ThietBiDienThu\\QLBH_Application\\src\\views\\SearchForProducts\\HomePage.vue"],"sourcesContent":["\r\n<style scoped>\r\n    .saved-button {\r\n        color: red !important; /* Màu chữ trắng */\r\n    }\r\n</style>\r\n<script>\r\nimport { Swiper, SwiperSlide } from 'swiper/vue';\r\n\r\n\r\n\r\n\r\nexport default {\r\n    data() {\r\n        return{\r\n            modules: [Navigation, Autoplay],\r\n        }\r\n    },\r\n    name: \"LANDING\",\r\n    components: {\r\n        Swiper,\r\n        SwiperSlide,\r\n        \r\n    },\r\n    computed:{\r\n    },\r\n    created(){\r\n\r\n    },\r\n    methods: {\r\n    },\r\n}\r\n</script>\r\n<template>\r\n    <v-container class=\"py-0\">\r\n        <swiper :navigation=\"true\" :autoplay=\"true\" :modules=\"modules\" :loop=\"true\" class=\"mySwiper\">\r\n            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-1.jpg\" alt=\"\"\r\n                    class=\"img-fluid\"></swiper-slide>\r\n            <swiper-slide><img src=\"@/assets/images/slider/img-slide-2.jpg\" alt=\"\"\r\n                    class=\"img-fluid\"></swiper-slide>\r\n            <swiper-slide> <img src=\"@/assets/images/slider/img-slide-3.jpg\" alt=\"\"\r\n                    class=\"img-fluid\"></swiper-slide>\r\n        </swiper>\r\n    </v-container>\r\n\r\n        <BContainerFluid class=\"container-fluid\">\r\n            <!-- <div class=\"container-fluid\"> -->\r\n            <div class=\"bg-dark mx-sm-3 home-section justify-content-center\" v-if=\"status\">\r\n               \r\n                <div class=\"swiper language-slides-hero m-0\">\r\n                    <div class=\"swiper-wrapper \" >\r\n                        <swiper class=\"default-swiper rounded\"  :autoplay=\"{\r\n                            delay: 2500,\r\n                            disableOnInteraction: false,\r\n                        }\">\r\n                            <swiper-slide>\r\n                                <BRow class=\"align-items-center justify-content-center text-center\">\r\n                                    <BCol sm=\"12\" class=\"header-content\">\r\n                                        <BRow class=\"justify-content-center text-center\">\r\n                                            <BCol lg=\"8\" sm=\"10\" Cols=\"11\" class=\"col-xl-7 col-md-9\">\r\n                                                <h1 class=\"my-3 wow animate__fadeInUp text-white\" data-wow-delay=\"0.4s\">Chào mừng bạn đến với trang web tìm kiếm nhà trọ.</h1>\r\n                                            </BCol>\r\n                                        </BRow>\r\n                                        <BRow class=\"justify-content-center text-center\">\r\n                                            <BCol lg=\"7\" sm=\"10\" Cols=\"11\" class=\"col-xxl-5 col-xl-6 col-md-8\">\r\n                                                <p class=\"f-16 mb-3 mb-lg-5 wow animate__fadeInUp\" data-wow-delay=\"0.6s\">Bạn vui lòng chọn địa chỉ trên thanh tìm kiếm để tìm phòng trọ phù hợp cho bạn.</p>\r\n                                            </BCol>\r\n                                        </BRow>\r\n                                    </BCol>\r\n                                </BRow>\r\n                            </swiper-slide>\r\n                        </swiper>\r\n                    </div>\r\n                </div>               \r\n            </div>\r\n            <div class=\"mx-sm-3 ListRoom\" v-else>\r\n                <v-container class=\"pt-0\">\r\n                    <v-row>\r\n                        <v-col md=\"1\"></v-col>\r\n                        <v-col cols=\"12\" md=\"10\">\r\n                            <h3>Danh sách các phòng trọ</h3>\r\n                            <BCard no-body v-for=\"(room, index) in roomData\" :key=\"index\">\r\n                                <BCardBody class=\"p-0\">\r\n                                    <BRow>\r\n                                        <BCol class=\"col-md-4 col-12\">\r\n                                            <v-window style=\"height: 100%;\"\r\n                                                v-model=\"room.selectimg\"\r\n                                                show-arrows\r\n                                                theme=\"dark\"\r\n                                                v-if=\"room.img != 0\"\r\n                                            >\r\n                                                <v-window-item\r\n                                                v-for=\"(item,index) in room.img\"\r\n                                                :key=\"index\"\r\n                                                >\r\n                                                <v-card class=\"d-flex justify-center align-center\" height=\"200px\">\r\n                                                    <v-img\r\n                                                        :src=\"item\"\r\n                                                        alt=\"Room Image\"\r\n                                                        aspect-ratio=\"16/9\"\r\n                                                        class=\"rounded elevation-2\"\r\n                                                        contain\r\n                                                    ></v-img>\r\n                                                </v-card>\r\n                                                </v-window-item>\r\n                                            </v-window>\r\n                                            <h4 v-else class=\"text-center\">Không có ảnh</h4>\r\n                                        </BCol>\r\n                                        <BCol class=\"col-md-8 col-12 mt-4\">\r\n                                            <router-link @click=\"GotoDetail(room.id)\" target=\"_blank\">\r\n                                                <h4> {{ room.towerName }}</h4>\r\n                                                <h5>Giá: <span style=\"color: red;\">{{ FormatPrice(room.price) }}</span></h5>\r\n                                                <p><v-icon>mdi-map-marker-radius</v-icon> {{ room.towerAddress }} </p>\r\n                                                <p>Thiết bị: {{ room.device }}</p>\r\n                                            </router-link>\r\n                                            <div class=\"text-right\">\r\n                                                <BButton variant=\"white\" :class=\"{'saved-button': room.isSave}\" @click=\"SaveRoom(room.id,room.isSave,index)\">\r\n                                                   <v-icon v-if=\"!room.isProcessing\">mdi-heart</v-icon>\r\n                                                    <v-progress-circular\r\n                                                    v-else\r\n                                                    indeterminate\r\n                                                    color=\"white\"\r\n                                                    size=\"20\"\r\n                                                    ></v-progress-circular>\r\n                                                </BButton>\r\n                                            </div>\r\n                                        </BCol>\r\n                                    </BRow>\r\n                                </BCardBody>\r\n                            </BCard>\r\n                        </v-col>\r\n                        <v-col xl=\"1\"></v-col>\r\n                    </v-row>\r\n                </v-container>\r\n            </div>\r\n\r\n        </BContainerFluid>\r\n</template>"],"mappings":"AAOA,SAASA,MAAM,EAAEC,WAAU,QAAS,YAAY;AAKhD,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAM;MACFC,OAAO,EAAE,CAACC,UAAU,EAAEC,QAAQ;IAClC;EACJ,CAAC;EACDC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACRP,MAAM;IACNC;EAEJ,CAAC;EACDO,QAAQ,EAAC,CACT,CAAC;EACDC,OAAOA,CAAA,EAAE,CAET,CAAC;EACDC,OAAO,EAAE,CACT;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}